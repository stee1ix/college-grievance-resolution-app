<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stee1ix.collegegrievancesystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeGrievanceSystem">
        <activity android:name=".TchrRegisterActivity"></activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".TchrListActivity" />
        <activity android:name=".TeacherLoginActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ReplyActivity" />
        <activity android:name=".StudentHistoryActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".TypeChooseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>